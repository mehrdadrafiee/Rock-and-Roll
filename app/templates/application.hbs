<div class="container">
  <small>app/templates/application.hbs</small>
  <div class="page-header">
    <h1>Rock & Roll<small>&nbsp;with Ember.js</small></h1>
  </div>
  <div class="row">
    <div class="col-md-6">
      <ul class="list-group">
        {{#each songs.sortedContent as |song|}}
          <li class="list-group-item">{{song.title}} by <em>{{song.band}}</em>
            <span class="rating pull-right">({{song.rating}})</span>
          </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>